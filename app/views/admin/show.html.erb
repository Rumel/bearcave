<h1>Admin Page</h1>

<h2>Upload Movies</h2>
<%= form_tag({action: :upload}, multipart: true) do %>
 	<%= file_field_tag 'file' %>
 	<%= submit_tag "Upload", class: "button" %>
<% end %>

<h2>Delete all Movies</h2>
<%= form_tag( admin_deleteall_path, method: "post", remote: true) do %>
	<button type="submit" class="button">Delete all Movies</button>
<% end %>
